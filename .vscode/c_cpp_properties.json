{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "YD_DEBUG"
            ],
            "cStandard": "c99",
            "cppStandard": "c++17",
            "forcedInclude": [],
            "macFrameworkPath": [
                "/Library/Frameworks",
                "/System/Library/Frameworks"
            ],
            "intelliSenseMode": "macos-clang-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ]
            },
            "compilerPath": "/usr/bin/clang",
            "compilerArgs": [
                "-I",
                ".vscode/cpp/include",
                "-include",
                "bits/stdc++.h",
                "-include",
                "leetcode.hpp",
                "-include",
                "leetcode-algorithm.hpp",
                "-include",
                "fast_io.hpp"
            ]
        }
    ],
    "version": 4
}